##########################
# component parameters

[component]

# the name of the component for the naming service
name ComponentRobotinoBaseServer

# the initial ComponentMode
initialComponentMode Neutral
#defaultScheduler FIFO
#useLogger true

##########################
# Task parameters

[OdomTask]
minActFreqHz 20.0
maxActFreqHz 1.0
# setup default trigger as PeriodicTimer
triggerType PeriodicTimer
periodicActFreqHz 50.0
# other trigger-types are:
#triggerType PeriodicTimer
#periodicActFreqHz 20.0
# or alternatively:
#triggerType DataTriggered
#inPortRef <InPortName>
#prescale 1
# optional scheduling parameters
# scheduler FIFO
# priority 0
# cpuAffinity 0

[SignalStateTask]
minActFreqHz 0.0
maxActFreqHz 0.0
# other trigger-types are:
#triggerType PeriodicTimer
#periodicActFreqHz 0.0
# or alternatively:
#triggerType DataTriggered
#inPortRef <InPortName>
#prescale 1
# optional scheduling parameters
# scheduler FIFO
# priority 0
# cpuAffinity 0

[robotinoAPITask]
minActFreqHz 0.0
maxActFreqHz 0.0
# other trigger-types are:
#triggerType PeriodicTimer
#periodicActFreqHz 0.0
# or alternatively:
#triggerType DataTriggered
#inPortRef <InPortName>
#prescale 1
# optional scheduling parameters
# scheduler FIFO
# priority 0
# cpuAffinity 0

##########################
# InputHandler parameters

[LocalizationEventServiceInHandler]
prescale 1

[LocalizationUpdateServiceInHandler]
prescale 1

[NavigationVelocityServiceInHandler]
prescale 1

[PowerOutputSendInHandler]
prescale 1

##########################
# server port parameter

[BaseStateQueryServiceAnsw]
serviceName BaseStateQueryServiceAnsw
roboticMiddleware ACE_SmartSoft

[BaseStateServiceOut]
