ComponentParameter ComponentWebotsMpsDocking component ComponentWebotsMpsDocking
{ 
    InternalParameter Webots
	{
      /** add an extra robot with this name and controller "<extern>" and supervisor TRUE */
      robotName : String = "MpsDocking";      
      /** dock to stations only if distance to the docking point is less than this value (in meters) */
      maxDistanceToDockingPoint : Double = 2.0;      
	}	
}
